  "scripts": {
    "build": "npm run -s build:clean && npm run -s build:webpack && npm run -s build:styleguide",
    "build:clean": "rm -rf ./dist",
    "build:webpack": "WEBPACK_ENV=production webpack-cli",
    "build:styleguide": "npx build-storybook -o dist/styleguide",
    "start:dev": "PORT=3000 WEBPACK_ENV=development webpack-dev-server --hot",
    "start:cypress": "PORT=3002 WEBPACK_ENV=development webpack-dev-server",
    "start:storybook": "npx start-storybook -p 3001",
    "start": "babel-node bin/app-server",
    "test": "NODE_ENV=test BABEL_ENV=coverage nyc mocha --opts test/unit/mocha.opts ./src/**/*.spec.js",
    "posttest": "npm run test:lint && npm run test:flow",
    "test:lint": "eslint --ignore-pattern src/app/container src test bin stories .storybook ./*.js",
    "test:flow": "flow",
    "test:dev": "NODE_ENV=test mocha --opts test/unit/mocha.opts ./src/**/*.spec.js",
    "test:dev:watch": "NODE_ENV=test mocha --opts test/unit/mocha.opts ./src/**/*.spec.js --watch",
    "test:cypress:run": "cypress run --record --key 29366e1c-0b4c-461a-a399-c99c42de27d2",
    "test:cypress": "start-server-and-test start:cypress 3002 test:cypress:run",
    "cypress:open:run": "cypress open",
    "cypress:open": "start-server-and-test start:cypress 3002 cypress:open:run",
    "changes": "./bin/update-changelog.js ./ --dry-run",
    "prerelease": "npm run test && ./bin/update-changelog.js ./",
    "release": "./bin/bump-release.js ./",
    "update-types": "flow-typed install",
    "prettier": "prettier --write \"**/*.js\""
  },

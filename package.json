  "main": "index.js",
  "scripts": {
    "build": "npm run -s build:clean && npm run -s build:webpack && npm run -s build:assets && npm run -s build:styleguide",
    "build:clean": "rm -rf ./dist",
    "build:webpack": "NODE_ENV=production webpack --config ./webpack/production.js",
    "build:assets": "mkdir -p dist/asset && rsync -rtvqL src/asset/ dist/asset",
    "build:styleguide": "build-storybook -c .storybook -o dist/styleguide",
    "start:dev": "PORT=3000 NODE_ENV=development babel-node bin/app-dev-server",
    "start:dev:stubs": "PORT=3000 NODE_ENV=development-with-stubs babel-node bin/app-dev-server",
    "start:styleguide": "start-storybook -p 3001 -s src",
    "start": "babel-node bin/app-server",
    "test": "npm run test:lint && npm run test:coverage",
    "test:lint": "eslint src test bin stories .storybook ./*.js",
    "test:dev": "NODE_ENV=test mocha --opts test/mocha.opts ./test/unit/ ./test/integration",
    "test:dev:watch": "NODE_ENV=test mocha --opts test/mocha.opts ./test/unit/ ./test/integration/ --watch",
    "test:coverage": "NODE_ENV=test babel-node node_modules/isparta/bin/isparta cover _mocha -- --opts test/mocha.opts",
    "changes": "./bin/update-changelog.js ./ --dry-run",
    "prerelease": "npm run test && ./bin/update-changelog.js ./",
    "release": "./bin/bump-release.js ./"
  },

{
  "check-coverage": true,
  "per-file": false,
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "statements": 100,
  "branches": 100,
  "functions": 100,
  "lines": 100,
  "include": [
    "src/**/*.js",
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.js",
    "src/app/*.js",
    "src/app/service/*.ts",
    "src/app/action/*.ts",
    "src/app/reducer/*.ts"
  ],
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "extension": [
    ".js",
    ".ts"
  ]
}

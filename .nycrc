{
    "check-coverage": true,
    "per-file": false,
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "lines": 100,
    "statements": 90,
    "branches": 86,
    "functions": 90,
    "lines": 90,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/app/*.js",
      "src/app/service/*.js"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "babel-register"
    ]
}
